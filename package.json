{
  "name": "imports-sorter",
  "displayName": "Imports Sorter",
  "version": "0.1.6",
  "publisher": "glhrmoura",
  "description": "Extension for Visual Studio Code that helps organize imports by sorting them based on their source folder.",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "import",
    "formatter",
    "javascript",
    "react",
    "react-native",
    "vue"
  ],
  "author": {
    "name": "Guilherme Moura <glhrmoura@gmail.com>"
  },
  "activationEvents": [
    "onCommand:importsSorter.sort"
  ],
  "icon": "src/images/icon.png",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/glhrmoura/imports-sorter.git"
  },
  "contributes": {
    "commands": [
      {
        "title": "Sort imports",
        "command": "importsSorter.sort"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "watch": "tsc -watch -p ./ --outDir ./dist"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.20.0",
    "typescript": "^4.7.4",
    "vsce": "^2.11.0"
  }
}
